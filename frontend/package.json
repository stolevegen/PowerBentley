    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "bits-ui": "^2.8.11",
    "clsx": "^2.1.1",
    "concurrently": "^8.0.0",
    "prettier": "^3.0.0",
    "svelte": "^5.0.0",
    "tailwind-merge": "^3.3.0",
    "tailwind-variants": "^1.0.0",
    "tw-animate-css": "^1.3.4",
    "vite": "^6.3.5",
    "vite-plugin-compression2": "^2.0.1"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.8",
    "lucide-svelte": "^0.513.0",
